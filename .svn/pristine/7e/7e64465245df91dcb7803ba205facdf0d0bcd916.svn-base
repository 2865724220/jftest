.regroupResultTop {
    margin-bottom: 0;
    background-image: linear-gradient(-134deg, #0C319D 0%, #0D2B7F 100%);
    box-shadow: 0 2px 10px 0 rgba(0,0,0,0.14);
}
.diagnoseResult  {
    margin-top: 20px;
}
.detail_leftWidth {
    width: 40%;
}
.detail_rightWidth {
    width: 60%;
    justify-content: space-around;
    align-items: flex-end;
}
.detail_rightWidth .resultRight2{
    flex: none;
    text-align: center;
}
.detail_rightWidth .resultRight2 .resultScore {
    line-height: 56px;
    height: 56px;
}
.detail_rightWidth .resultRight2 .tong, .detail_leftWidth .tong {
    color: #6a7c98;
}
.regroupdiagTop {
    background: #fff;
    border-top: solid 1px #ccc;
    align-items: center;
}
.regroupdiagTop .diagTopRight {
    padding-top: 0; 
    width: 33%;
    height: 40px;
    border-right: 1px solid #ccc; 
}
.regroupdiagTop .diagTopRight:last-of-type {
    border-right: none;
}